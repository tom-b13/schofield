{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/schemas/AutosaveResult.schema.json",
  "title": "AutosaveResult",
  "type": "object",
  "properties": {
    "saved": { "type": "boolean" },
    "etag": { "type": "string" },
    "suppressed_answers": {
      "type": "array",
      "items": { "type": "string", "format": "uuid" }
    },
    "visibility_delta": {
      "type": "object",
      "properties": {
        "now_visible": {
          "type": "array",
          "items": { "type": "string", "format": "uuid" }
        },
        "now_hidden": {
          "type": "array",
          "items": { "type": "string", "format": "uuid" }
        }
      }
    }
  }
}

