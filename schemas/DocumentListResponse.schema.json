{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "schemas/DocumentListResponse.schema.json",
  "title": "DocumentListResponse",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "list",
    "list_etag"
  ],
  "properties": {
    "list_etag": {
      "type": "string",
      "minLength": 1
    },
    "list": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": [
          "document_id",
          "title",
          "order_number",
          "version"
        ],
        "properties": {
          "document_id": {
            "$ref": "./document_id.schema.json"
          },
          "title": {
            "$ref": "./Document.schema.json#/properties/title"
          },
          "order_number": {
            "type": "integer",
            "minimum": 1
          },
          "version": {
            "type": "integer",
            "minimum": 1
          }
        }
      }
    }
  }
}
