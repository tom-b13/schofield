{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "schemas/Outputs.schema.json",
  "title": "Outputs",
  "type": "object",
  "description": "Canonical authoring operation outputs container.",
  "properties": {
    "screen": {
      "type": "object",
      "description": "Projection of the affected screen for screen operations.",
      "properties": {
        "screen_id": {
          "$ref": "./ScreenId.schema.json"
        },
        "title": {
          "$ref": "./ScreenTitle.schema.json"
        },
        "screen_order": {
          "$ref": "./ScreenOrder.schema.json"
        }
      },
      "required": [
        "screen_id",
        "title",
        "screen_order"
      ],
      "additionalProperties": false
    },
    "question": {
      "type": "object",
      "description": "Projection of the affected question for question operations.",
      "properties": {
        "question_id": {
          "$ref": "./question_id.schema.json"
        },
        "screen_id": {
          "$ref": "./ScreenId.schema.json"
        },
        "question_text": {
          "$ref": "./QuestionText.schema.json"
        },
        "answer_kind": {
          "$ref": "./AnswerKind.json"
        },
        "question_order": {
          "$ref": "./QuestionOrder.schema.json"
        },
        "parent_question_id": {
          "$ref": "./question_id.schema.json"
        },
        "visible_if_value": {
          "$ref": "./VisibleIfValue.schema.json"
        }
      },
      "required": [
        "question_id",
        "screen_id",
        "question_text",
        "question_order"
      ],
      "additionalProperties": false
    },
    "etags": {
      "type": "object",
      "description": "Entity tag projection for optimistic concurrency.",
      "properties": {
        "question": {
          "$ref": "./ETag.schema.json"
        },
        "screen": {
          "$ref": "./ETag.schema.json"
        },
        "questionnaire": {
          "$ref": "./ETag.schema.json"
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}