{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/schemas/RegenerateCheckResult.schema.json",
  "title": "RegenerateCheckResult",
  "type": "object",
  "properties": {
    "ok": {
      "type": "boolean",
      "description": "True if generation may proceed"
    },
    "blocking_items": {
      "type": "array",
      "description": "List of unmet mandatory items that prevent generation",
      "items": {
        "type": "object",
        "properties": {
          "question_id": {
            "$ref": "https://example.com/schemas/QuestionId.schema.json"
          },
          "reason": {
            "type": "string"
          }
        },
        "required": [
          "question_id",
          "reason"
        ],
        "additionalProperties": false
      }
    }
  },
  "required": [
    "ok",
    "blocking_items"
  ],
  "additionalProperties": false
}